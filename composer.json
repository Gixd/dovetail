{
  "name": "gixd/dovetail",
  "description": "A Laravel 5 wrapper for the Teamwork API",
  "type": "library",
  "license": "CC-BY-NC-4.0",
  "authors": [
    {
      "name": "Zachary Schuessler",
      "email": "zlschuessler@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SquareBit\\Dovetail\\": "src/",
      "SquareBit\\Dovetail\\Commands\\": "commands/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SquareBit\\Dovetail\\Test\\": "tests/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "~7.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SquareBit\\Dovetail\\ServiceProvider"
      ],
      "aliases": {
        "Dovetail": "SquareBit\\Dovetail\\Facade"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  }
}
